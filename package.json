{
  "name": "roguelike",
  "version": "0.0.0",
  "description": "A Free HTML5 Roguelike",
  "main": "index.js",
  "scripts": {
    "install": "webpack",
    "start": "node src/server/server.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive --compilers js:babel-register --colors --reporter dot && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "npm run cover && istanbul check-coverage --statements 30 --branches 30 --functions 30 ./coverage/coverage.raw.json",
    "develop": "webpack-dev-server --port 1337",
    "doc": "esdoc -c esdoc.json"
  },
  "author": "Alex Babis <acbabis@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel": "6.3.13",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.0",
    "chai": "^3.5.0",
    "compression": "^1.6.2",
    "coveralls": "^2.11.12",
    "css-loader": "^0.23.1",
    "dialog-polyfill": "^0.4.3",
    "es6-a-star": "git+https://github.com/acbabis/es6-a-star.git#master",
    "es6-module-loader": "0.17.8",
    "express": "4.13.4",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "isparta": "^4.0.0",
    "istanbul": "git+https://github.com/gotwarlost/istanbul.git#v1",
    "jquery": "^2.2.4",
    "jsdom": "^9.3.0",
    "mocha": "^2.3.4",
    "node-sass": "^3.7.0",
    "normalize.css": "^4.1.1",
    "random-js": "^1.0.8",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "traceur": "0.0.93",
    "typescript": "1.7.3",
    "url-loader": "^0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "^1.13.0"
  },
  "devDependencies": {
    "esdoc": "^0.4.8",
    "jsdoc": "^3.4.0",
    "jsdoc-baseline": "https://github.com/hegemonic/baseline/tarball/master"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acbabis/roguelike.git"
  },
  "keywords": [
    "html5",
    "roguelike"
  ],
  "bugs": {
    "url": "https://github.com/acbabis/roguelike/issues"
  },
  "homepage": "https://github.com/acbabis/roguelike#readme"
}
