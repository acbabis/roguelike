{
  "name": "roguelike",
  "version": "0.0.0",
  "description": "A Free HTML5 Roguelike",
  "main": "index.js",
  "engines": {
    "node": ">=6.10.3"
  },
  "scripts": {
    "install": "npm run build",
    "build": "webpack",
    "start": "babel-node src/server/server.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive --compilers js:babel-register --colors --reporter dot",
    "cover:upload": "cat ./coverage/lcov.info && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "npm run cover && istanbul check-coverage --statements 30 --branches 30 --functions 30 ./coverage/coverage.raw.json",
    "doc": "esdoc -c esdoc.json"
  },
  "author": "Alex Babis <acbabis@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.6.2",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.16.1",
    "chai": "^4.1.1",
    "compression": "^1.6.2",
    "coveralls": "^2.11.16",
    "css-loader": "^0.28.1",
    "dialog-polyfill": "^0.4.7",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.28.0",
    "isparta": "^4.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^11.1.0",
    "mocha": "^3.0.2",
    "mysql": "^2.13.0",
    "node-sass": "^4.5.0",
    "normalize.css": "^7.0.0",
    "pixi.js": "^4.5.0",
    "random-js": "^1.0.8",
    "sass-loader": "^6.0.5",
    "sinon": "^3.2.1",
    "style-loader": "^0.17.0",
    "traceur": "0.0.111",
    "url-loader": "^0.5.7",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-spritesmith": "^0.3.1"
  },
  "devDependencies": {
    "esdoc": "^1.0.1",
    "eslint": "^4.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acbabis/roguelike.git"
  },
  "keywords": [
    "html5",
    "roguelike"
  ],
  "bugs": {
    "url": "https://github.com/acbabis/roguelike/issues"
  },
  "homepage": "https://github.com/acbabis/roguelike#readme"
}
