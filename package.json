{
  "name": "roguelike",
  "version": "0.0.0",
  "description": "A Free HTML5 Roguelike",
  "main": "index.js",
  "scripts": {
    "install": "webpack",
    "start": "node src/server/server.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha -- --recursive --compilers js:babel-register --colors --reporter dot",
    "cover:upload": "cat ./coverage/lcov.info && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test": "npm run cover && istanbul check-coverage --statements 30 --branches 30 --functions 30 ./coverage/coverage.raw.json",
    "develop": "webpack-dev-server --port 1337",
    "doc": "esdoc -c esdoc.json"
  },
  "author": "Alex Babis <acbabis@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.8",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.0",
    "chai": "^3.5.0",
    "compression": "^1.6.2",
    "coveralls": "^2.11.15",
    "css-loader": "^0.23.1",
    "dialog-polyfill": "^0.4.5",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.17.0",
    "isparta": "^4.0.0",
    "istanbul": "git+https://github.com/gotwarlost/istanbul.git#v1",
    "jquery": "^2",
    "jsdom": "^9.3.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.13.0",
    "normalize.css": "^4.1.1",
    "random-js": "^1.0.8",
    "sass-loader": "^4.0.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "traceur": "0.0.111",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "devDependencies": {
    "esdoc": "^0.4.8"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acbabis/roguelike.git"
  },
  "keywords": [
    "html5",
    "roguelike"
  ],
  "bugs": {
    "url": "https://github.com/acbabis/roguelike/issues"
  },
  "homepage": "https://github.com/acbabis/roguelike#readme"
}
